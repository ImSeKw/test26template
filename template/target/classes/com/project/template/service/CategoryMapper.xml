<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.project.template.service.CategoryMapper">
	<select id="selectCategoryList" resultType="com.project.template.service.Category">
		SELECT 
			category_no AS categoryNo
			, category_name AS categoryName
		FROM category
	</select>
</mapper>